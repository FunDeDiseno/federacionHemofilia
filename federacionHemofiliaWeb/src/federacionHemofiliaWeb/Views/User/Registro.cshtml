        <br/><br/>

    <div class="container">

      <form class="form-signin">

        <h2 class="form-signin-heading">Traveler Name </h2>
        <label for="inputEmail" class="sr-only">Traveler Name</label>
        <input id="traveler_name" class="form-control" placeholder="Traveler Name" required autofocus>
        <div class="checkbox">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
        </div>
        <h2 class="form-signin-heading">Travel Code</h2>
        <label for="inputEmail" class="sr-only">Travel Code</label>
        <input id="code_trip" class="form-control" placeholder="Travel Code (XX12X)" required autofocus>
        <div class="checkbox">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
        </div>
        <br/>
        <button id = "go-traveler" class="btn btn-lg btn-primary btn-block" onclick="saveTraveler()" type="submit">Let's Travel</button>
      </form>

    </div> <!-- /container -->


