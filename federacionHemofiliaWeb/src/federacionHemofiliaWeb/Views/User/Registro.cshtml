@{ 
    ViewData["Title"] = "Registro Doctor";
}
 <body class="sign-in">
     
    <div class="container">
      <form class="form-signin">
        <h1 class="text-center">Registro Doctor </h1>  
        <!--<h3 class="form-signin-heading">Nombre(s)</h3>-->
        <!-- Stack the columns on mobile by making one full-width and the other half-width -->
        <div class="row">
            <div class="col-xs-12 col-lg-6">
                <label for="inputEmail" class="sr-only">Nombre(s)</label>
                <input id="Doctor-names" class="form-control" placeholder="Nombre(s)" required autofocus>
            </div>
            <div class="col-xs-12 col-lg-6">
                <label for="inputEmail" class="sr-only">Apellidos</label>
                <input id="Doctor-lastnames" class="form-control" placeholder="Apellidos" required autofocus>
            </div>
        </div>
        <br/>
        <div>
        <label for="inputEmail" class="sr-only">Correo Electronico</label>
        <input type="email" id="email" class="form-control" placeholder="Correo Electronico" required autofocus>
        </div>
        <br/>
        <div class="row">
            <div class="col-xs-12 col-lg-6">
                <label for="inputEmail" class="sr-only">Contrase単a</label>
                <input type="password" id="passw" class="form-control" placeholder="Contrase単a" required autofocus>
            </div>
            <div class="col-xs-12 col-lg-6">
                <label for="inputEmail" class="sr-only">Validar Contrase単a</label>
                <input type="password" id="passw" class="form-control" placeholder="Validar Contrase単a" required autofocus>
            </div>
        </div>
        <br/>
        <div>
        <label for="inputEmail" class="sr-only">Especialidad</label>
        <input id="especialidad" class="form-control" placeholder="Especialidad" required autofocus>
        </div>
        
        <div class="checkbox">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
        </div>
        <br/>
        <button id = "go-traveler" class="btn btn-lg btn-primary btn-block" onclick="saveTraveler()" type="submit">Registro</button>
      </form>

    </div> <!-- /container -->


